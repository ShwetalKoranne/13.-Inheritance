#include<iostream>
using namespace std;

class Student{
    public:

    int prn;
    string name;
};

class Subject{
    public:

    string sub;
    int marks;
};

class Profile: public Student, public Subject{
    public:

    char div;
};

int main(){

    Profile p1;
    p1.name="Shwetal";
    p1.prn=106;
    p1.div='B';
    p1.sub="CDS";
    p1.marks=91;

    cout<<"Name: "<<p1.name<<endl;
    cout<<"PRN: "<<p1.prn<<endl;
    cout<<"Division: "<<p1.div<<endl;
    cout<<"Subject: "<<p1.sub<<endl;
    cout<<"Marks: "<<p1.marks<<endl;
    
    return 0;
}


/*        OUTPUT 
Name: Shwetal
PRN: 106
Division: B
Subject: CDS
Marks: 91
*/
